{
    "whoAmI.backup_who_am_i.html": "<!DOCTYPE html>\r\n<html lang=\"en\" id=\"html-root\">\r\n<head id=\"head-section\">\r\n    <meta charset=\"UTF-8\" id=\"charset-meta\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" id=\"viewport-meta\">\r\n    <title id=\"page-title\">Who Am I? - IP Information Tool</title>\r\n    <link rel=\"stylesheet\" href=\"who_am_i.css\" id=\"main-stylesheet\">\r\n</head>\r\n<body id=\"main-body\">\r\n    <header id=\"main-header\">\r\n        <h1 id=\"main-heading\">Who Am I?</h1>\r\n        <p id=\"subtitle\">Discover your public IP and location information</p>\r\n    </header>\r\n\r\n    <main id=\"main-content\">\r\n        <!-- IP Address Section -->\r\n        <section id=\"ip-section\" class=\"card\">\r\n            <h2 id=\"ip-section-title\">Your IP Address</h2>\r\n            <div id=\"ip-container\">\r\n                <button id=\"get-ip-btn\" class=\"primary-btn\">Find My IP Address</button>\r\n                <div id=\"ip-display\" class=\"hidden\">\r\n                    <p id=\"ip-label\">Your Public IP:</p>\r\n                    <span id=\"ip-address\" class=\"highlight-text\"></span>\r\n                </div>\r\n                <div id=\"ip-loading\" class=\"loading hidden\">\r\n                    <div id=\"ip-spinner\" class=\"spinner\"></div>\r\n                    <p id=\"ip-loading-text\">Getting your IP address...</p>\r\n                </div>\r\n            </div>\r\n        </section>\r\n\r\n        <!-- IP Details Section -->\r\n        <section id=\"details-section\" class=\"card hidden\">\r\n            <h2 id=\"details-section-title\">Your Location Details</h2>\r\n            <div id=\"details-container\">\r\n                <button id=\"get-details-btn\" class=\"secondary-btn\">Get Location Details</button>\r\n                <div id=\"details-display\" class=\"hidden\">\r\n                    <div id=\"location-info\" class=\"info-grid\">\r\n                        <div id=\"city-info\" class=\"info-item\">\r\n                            <label id=\"city-label\">City:</label>\r\n                            <span id=\"city-value\" class=\"info-value\"></span>\r\n                        </div>\r\n                        <div id=\"region-info\" class=\"info-item\">\r\n                            <label id=\"region-label\">Region:</label>\r\n                            <span id=\"region-value\" class=\"info-value\"></span>\r\n                        </div>\r\n                        <div id=\"country-info\" class=\"info-item\">\r\n                            <label id=\"country-label\">Country:</label>\r\n                            <span id=\"country-value\" class=\"info-value\"></span>\r\n                        </div>\r\n                        <div id=\"timezone-info\" class=\"info-item\">\r\n                            <label id=\"timezone-label\">Timezone:</label>\r\n                            <span id=\"timezone-value\" class=\"info-value\"></span>\r\n                        </div>\r\n                        <div id=\"isp-info\" class=\"info-item\">\r\n                            <label id=\"isp-label\">ISP:</label>\r\n                            <span id=\"isp-value\" class=\"info-value\"></span>\r\n                        </div>\r\n                        <div id=\"coordinates-info\" class=\"info-item\">\r\n                            <label id=\"coordinates-label\">Coordinates:</label>\r\n                            <span id=\"coordinates-value\" class=\"info-value\"></span>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <div id=\"details-loading\" class=\"loading hidden\">\r\n                    <div id=\"details-spinner\" class=\"spinner\"></div>\r\n                    <p id=\"details-loading-text\">Getting your location details...</p>\r\n                </div>\r\n            </div>\r\n        </section>\r\n\r\n        <!-- Error Section -->\r\n        <section id=\"error-section\" class=\"card error-card hidden\">\r\n            <h2 id=\"error-title\">Oops! Something went wrong</h2>\r\n            <p id=\"error-message\"></p>\r\n            <button id=\"retry-btn\" class=\"primary-btn\">Try Again</button>\r\n        </section>\r\n    </main>\r\n\r\n    <footer id=\"main-footer\">\r\n        <p id=\"footer-text\">Powered by IPify and IPinfo APIs</p>\r\n        <p id=\"privacy-note\">Your data is not stored and is only used for display purposes.</p>\r\n    </footer>\r\n\r\n    <script src=\"who_am_i.js\" id=\"main-script\"></script>\r\n</body>\r\n</html>"
}